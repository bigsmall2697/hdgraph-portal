{
    "name": "myhbarwallet",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "npm-run-all lint:js lint:css",
        "lint:js": "vue-cli-service lint",
        "lint:css": "stylelint --fix 'src/**/*.vue' 'src/**/*.css'",
        "test": "npm-run-all test:unit",
        "test:unit": "vue-cli-service test:unit",
        "test:e2e": "vue-cli-service test:e2e",
        "firebase:deploy:testnet": "firebase deploy --only hosting:testnet",
        "firebase:deploy:mainnet": "firebase deploy --only hosting:mainnet",
        "deploy:testnet": "cross-env HEDERA_NETWORK=testnet npm-run-all test:unit build firebase:deploy:testnet",
        "deploy:mainnet": "cross-env HEDERA_NETWORK=mainnet npm-run-all test:unit build firebase:deploy:mainnet"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.22",
        "@fortawesome/free-solid-svg-icons": "^5.10.2",
        "@fortawesome/vue-fontawesome": "^0.1.7",
        "@hashgraph/sdk": "^0.3.2",
        "@mdi/js": "^4.2.95",
        "@vue/composition-api": "^0.2.1",
        "bignumber.js": "^9.0.0",
        "html2pdf": "^0.0.11",
        "jdenticon": "^2.2.0",
        "qrcode.vue": "^1.6.2",
        "ua-parser-js": "^0.7.20",
        "vue": "^2.6.10",
        "vue-router": "^3.1.3",
        "vuex": "^3.1.1",
        "zxcvbn": "^4.4.2"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@improbable-eng/grpc-web-node-http-transport": "^0.11.0",
        "@launchbadge/eslint-config": "^0.11.5",
        "@types/core-js": "2.x.x",
        "@types/jest": "^24.0.18",
        "@types/prettier": "1.18.2",
        "@types/ua-parser-js": "^0.7.33",
        "@types/zxcvbn": "^4.4.0",
        "@vue/cli-plugin-babel": "^4.0.0-rc.1",
        "@vue/cli-plugin-e2e-nightwatch": "^3.11.0",
        "@vue/cli-plugin-eslint": "^4.0.0-rc.1",
        "@vue/cli-plugin-typescript": "^4.0.0-rc.1",
        "@vue/cli-plugin-unit-jest": "^4.0.0-rc.1",
        "@vue/cli-service": "^4.0.0-rc.1",
        "@vue/test-utils": "1.0.0-beta.29",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.0.3",
        "chromedriver": "^76.0.1",
        "cross-env": "^5.2.1",
        "cssnano-preset-advanced": "^4.0.7",
        "eslint": "^6.3.0",
        "firebase-tools": "^7.3.1",
        "image-webpack-loader": "^6.0.0",
        "jest": "^24.9.0",
        "jest-canvas-mock": "^2.1.1",
        "lint-staged": "^9.2.5",
        "npm-run-all": "^4.1.5",
        "postcss-font-magician": "^2.2.2",
        "postcss-normalize": "^8.0.1",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^1.18.2",
        "stylelint": "^10.1.0",
        "stylelint-a11y": "^1.2.1",
        "stylelint-config-standard": "^18.3.0",
        "stylelint-csstree-validator": "^1.5.2",
        "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
        "stylelint-declaration-strict-value": "^1.1.3",
        "stylelint-no-indistinguishable-colors": "^1.2.1",
        "stylelint-no-unsupported-browser-features": "^3.0.2",
        "stylelint-no-unused-selectors": "^1.0.11",
        "stylelint-order": "^3.1.0",
        "stylelint-prettier": "^1.1.1",
        "stylelint-use-logical": "^1.1.0",
        "stylelint-value-no-unknown-custom-properties": "^2.0.0",
        "ts-jest": "^24.0.2",
        "typescript": "^3.6.2",
        "vue-template-compiler": "^2.6.10"
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,vue}": [
            "vue-cli-service lint",
            "stylelint --fix 'src/**/*.vue' 'src/**/*.css'",
            "git add"
        ],
        "*.css": [
            "stylelint --fix 'src/**/*.css'",
            "git add"
        ]
    }
}
